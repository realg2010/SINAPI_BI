CREATE PROCEDURE spCURRENCY
(
 IN pCURRENCY_ID VARCHAR(5),IN pCURRENCY_NAME VARCHAR(100),IN pREPORT_DATE VARCHAR(20),IN pDB_DATE VARCHAR(20)
 )
LANGUAGE SQL
 BEGIN
  INSERT INTO CURRENCY (CURRENCY_ID,CURRENCY_NAME,REPORT_DATE,DB_DATE)
					VALUES (pCURRENCY_ID,pCURRENCY_NAME,pREPORT_DATE,pDB_DATE);
 END
