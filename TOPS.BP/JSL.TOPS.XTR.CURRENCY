CREATE TABLE DimCURRENCY
(
 CURRENCYDWKEY INT IDENTITY PRIMARY KEY,
 CURRENCY_ID VARCHAR(5),
 CURRENCY_NAME VARCHAR(100),
 CURRENT_FLAG BIT DEFAULT 1
)
