SUBROUTINE TEST.PAYMENT
$INSERT I_COMMON
$INSERT I_EQUATE
$INSERT I_F.AA.PAYMENT.SCHEDULE


DEBUG
        ARR.ID="AA18023K3ZJ7"
	CALL AA.GET.ARRANGEMENT.CONDITIONS(ARR.ID, 'PAYMENT.SCHEDULE', '', TODAY, SCH.PROP.ID, SCH.PROP.REC, ER)
	SCH.REC=RAISE(SCH.PROP.REC)
	CALC.AMT=SCH.REC<AA.PS.CALC.AMOUNT>
	ACT.AMT=SCH.REC<AA.PS.ACTUAL.AMT>
	IF NOT(CALC.AMT) THEN
	VAL.AMT=ACT.AMT
	END ELSE
	 VAL.AMT=CALC.AMT
	END
RETURN
END